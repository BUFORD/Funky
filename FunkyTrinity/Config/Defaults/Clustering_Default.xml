<?xml version="1.0" encoding="utf-8" ?>
<SettingCluster xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ClusterDistance>10</ClusterDistance>
  <ClusterMinimumUnitCount>2</ClusterMinimumUnitCount>
  <EnableClusteringTargetLogic>true</EnableClusteringTargetLogic>
  <ClusterKillLowHPUnits>false</ClusterKillLowHPUnits>
  <ClusteringAllowRangedUnits>true</ClusteringAllowRangedUnits>
  <ClusteringAllowSpawnerUnits>true</ClusteringAllowSpawnerUnits>
  <IgnoreClusteringWhenLowHP>true</IgnoreClusteringWhenLowHP>
  <IgnoreClusterLowHPValue>0.50</IgnoreClusterLowHPValue>
</SettingCluster>